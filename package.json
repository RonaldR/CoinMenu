{
  "name": "coin-menu",
  "version": "0.1.1",
  "description": "Cryptocoins in your menubar",
  "repository": {
    "type": "git",
    "url": "https://github.com/RonaldR/CoinMenu.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "electron-compile": "^6.4.1",
    "menubar": "^5.2.3",
    "moment": "^2.20.1",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vue2-filters": "^0.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.10",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.10",
    "@vue/cli-service": "^3.0.0-alpha.10",
    "electron-prebuilt-compile": "1.8.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "main": "src/electron-main.js",
  "scripts": {
    "start": "electron .",
    "start-forge": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "RonaldR",
        "name": "CoinMenu",
        "prerelease": true,
        "draft": false
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
